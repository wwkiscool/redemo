  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "devDependencies": {
    "cssnano-preset-advanced": "^4.0.7",
